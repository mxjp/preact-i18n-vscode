{
	"name": "preact-i18n-vscode",
	"displayName": "preact-i18n-vscode",
	"private": true,
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/index.js",
	"contributes": {
		"commands": [
			{
				"command": "preact-i18n.edit",
				"category": "Localization",
				"title": "Edit Translations"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile:ext": "tsc -p tsconfig-ext.json",
		"compile:editor": "webpack --config webpack.editor.js",
		"compile": "concurrently npm:compile:ext npm:compile:editor",
		"watch:ext": "tsc -p tsconfig-ext.json -w",
		"watch:editor": "webpack --config webpack.editor.js --watch",
		"watch": "concurrently npm:watch:ext npm:watch:editor",
		"pretest": "npm run compile"
	},
	"devDependencies": {
		"@types/json5": "0.0.30",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.45.0",
		"concurrently": "^5.2.0",
		"preact": "^10.4.4",
		"ts-loader": "^7.0.5",
		"typescript": "^3.6.4",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"@mpt/preact-i18n": "^0.1.10",
		"json5": "^2.1.3"
	},
	"repository": "https://github.com/mxjp/preact-i18n-vscode.git",
	"author": "Max J. Polster",
	"license": "MIT"
}
